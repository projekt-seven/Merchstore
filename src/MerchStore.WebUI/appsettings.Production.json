{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "${CONNECTION_STRING}"
  },
  "ApiKey": {
    "Value": "${API_KEY}"
  },
  "AdminUser": {
    "Username": "${ADMIN_USERNAME}",
    "Password": "${ADMIN_PASSWORD}",
    "Email": "${ADMIN_EMAIL}",
    "AdminRole": "${ADMIN_ROLE}"
  }
}

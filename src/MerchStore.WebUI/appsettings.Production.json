{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "${{ secrets.AZURE_SQL_DB_CONNECTIONSTRING }}"
  },
  "ApiKey": {
    "Value": "${{ secrets.BASIC_PRODUCT_API_KEY }}"
  },
  "AdminUser": {
    "Username": "${{ secrets.ADMIN_USER_USERNAME }}",
    "Password": "${{ secrets.ADMIN_USER_PASSWORD }}",
    "Email": "${{ secrets.ADMIN_USER_EMAIL }}",
    "AdminRole": "${{ secrets.ADMIN_USER_ROLE }}"
  }
}
